{
  "name": "flasher",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.1",
    "axios": "^0.16.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "better-react-spinkit": "^2.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "^0.28.5",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "material-ui": "^0.18.7",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-cookie": "^2.0.8 ",
    "react-css-themr": "^2.1.2",
    "react-dev-utils": "^3.1.1",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.10",
    "react-flexbox-grid": "^1.1.4",
    "react-grid-system": "^2.9.6",
    "react-material-icons": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.12",
    "react-toolbox-themr": "^1.0.2",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-persist": "^4.9.1",
    "redux-test-utils": "^0.1.3",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "chai": "^4.1.2",
    "chai-spies": "^0.7.1",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3"
  },
  "reactToolbox": {
    "include": [
      "APP_BAR",
      "BUTTON",
      "CARD",
      "CHECKBOX",
      "DRAWER",
      "DROPDOWN",
      "LAYOUT",
      "LINK",
      "DATE_PICKER"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-primary": "#1976D2",
      "color-accent": "#9be7ff",
      "color-accent-dark": "#2286c3",
      "color-primary-contrast": "#fafafa",
      "color-accent-contrast": "#ffffff"
    },
    "output": "src/react-toolbox"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "toolbox": "react-toolbox-themr",
    "test": "jest",
    "testw": "jest --watch --env=jsdom",
    "testc": "jest --coverage",
    "testwc": "jest --watch --env=jsdom --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
